<rxui:ReactiveUserControl
    x:Class="ReactiveUIPlay.OrderItemControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ReactiveUIPlay"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rxui="clr-namespace:ReactiveUI;assembly=ReactiveUI.Wpf"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:TypeArguments="local:OrderItemViewModel"
    mc:Ignorable="d">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBlock>Product</TextBlock>
            <TextBlock Name="Product" Margin="10,0,0,0" />
        </StackPanel>
    </Grid>
</rxui:ReactiveUserControl>
